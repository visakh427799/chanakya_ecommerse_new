        <h2 class="text-center">Register</h2>
        <section class="contact-area ptb-70">
            
            
            <div class="container">
                
                <div class="row">
                    <div class="col-lg-7 col-md-12">
                        <div class="contact-form">
                         <form action="/signup" method="POST">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12">
                                        <div class="form-group">
                                            <input type="text" name="username" id="name" class="form-control" required data-error="Please enter your name" placeholder="Name">
                                            <div class="help-block with-errors"></div>
                                        </div>
                                    </div>

                                    <div class="col-lg-6 col-md-6">
                                        <div class="form-group">
                                            <input type="email" name="email" id="email" class="form-control" required data-error="Please enter your email" placeholder="Email">
                                            <div class="help-block with-errors"></div>
                                        </div>
                                    </div>

                                    <div class="col-lg-6 col-md-6">
                                        <div class="form-group">
                                            <input type="text" name="phone" id="phone_number" required data-error="Please enter your number" class="form-control" placeholder="Phone">
                                            <div class="help-block with-errors"></div>
                                        </div>
                                    </div>

                                     <div class="col-lg-6 col-md-6">
                                        <div class="form-group">
                                             <input type="password" name="pass1" id="phone_number" required data-error="Please enter your password" class="form-control" placeholder="Password">
                                            <div class="help-block with-errors"></div>
                                        </div>
                                    </div>

                                    <div class="col-lg-6 col-md-6">
                                        <div class="form-group">
                                             <input type="password" name="pass2" id="phone_number" required data-error="Please confirm your password" class="form-control" placeholder="Confirm password">
                                            <div class="help-block with-errors"></div>
                                        </div>
                                    </div>
                                    
                                          


                                    <div class="col-lg-12 col-md-12">
                                        <button type="submit" class="btn btn-primary">Signup <i class="flaticon-right-chevron"></i></button>
                                        
                                    </div>
                                </div>
                            </form>
                           
                           <h5 class="text-danger">{{message}}</h5>


                        </div>
                    </div>

