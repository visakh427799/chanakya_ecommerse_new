<section>
    <center><h3 class="text-info">ALL ORDERS</h3></center>
    <div class="row m-3">
        <table class="table">
            <thead class=" table-dark">
              <tr>
                <th scope="col">Name</th>
                <th scope="col">Mobile</th>
                <th scope="col">Pincode</th>
                <th scope="col">Order id</th>
                
                <th scope="col">Total prize</th>
                <th scope="col">Payment method</th>
                <th scope="col">Payment status</th>
                <th scope="col">order status</th>
              </tr>
            </thead>
            <tbody>

{{#each data}}

              <tr>
               
                <td>{{this.Address.user_name}}</td>
                <td>{{this.Address.user_phone}}</td>
                <td>{{this.Address.user_pincode}}</td>
                <td>{{this._id}}</td>
               
                <td>{{this.Total}}</td>
                <td>{{this.Payment_method}}</td>
                <td>{{this.Payment_status}}</td>
                <td>{{this.Order_status}}</td>
               

                 


              </tr>
            
       {{/each}}
            </tbody>
          </table>

    </div>
