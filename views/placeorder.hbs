    <section class="contact-area ptb-70">


        <div class="container">

            <div class="row mt-4">

                <div class="col-lg-7 col-md-12">
                    <div class="contact-form">
                        <form action="/placeorder" method="POST">
                            <div class="row">

                                <div class="col-lg-6 col-md-6">

                                    <div class="form-group">
                                        <label for="inputEmail4" class="text-warning">Name</label>
                                        <input type="text" name="username" class="form-control" required
                                            placeholder="Please enter your name">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6">

                                    <div class="form-group">
                                        <label for="inputEmail4" class="text-warning"> Pincode</label>
                                        <input type="text" name="userpin" class="form-control" required
                                            placeholder="Please enter your pincode">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">

                                    <div class="form-group">
                                        <label for="inputEmail4" class="text-warning">Post Office</label>
                                        <input type="text" name="userpostoffice" id="phone_number" required
                                            placeholder="Post office" class="form-control">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">

                                    <div class="form-group">
                                        <label for="inputEmail4" class="text-warning">Phone</label>
                                        <input type="text" name="userphone" id="email" class="form-control" required
                                            placeholder="Please enter your phone">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">

                                    <div class="form-group">
                                        <label for="inputEmail4" class="text-warning">State</label>
                                        <select name="userstate" class="form-control">
                                            {{#each dat}}
                                            <option value="{{this.d}}">{{this.d}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">

                                    <div class="form-group">
                                        <label for="inputEmail4" class="text-warning">District</label>
                                        <input type="text" name="userdistrict" id="phone_number" required
                                            placeholder="Please enter your district" class="form-control">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">

                                    <div class="form-group">
                                        <label for="inputEmail4" class="text-warning">City/Locality</label>
                                        <input type="password" name="usercity" id="phone_number" required
                                            placeholder="Please enter your locality and city" class="form-control">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">

                                    <div class="form-group">
                                        <label for="inputEmail4" class="text-warning">House name</label>
                                        <input type="text" name="userhouse" id="phone_number" required
                                            placeholder="House name" class="form-control">
                                        <div class="help-block with-errors"></div>
                                    </div>
                                </div>





                                {{!-- <input type="text" class="form-control" name="inputCategory" required> --}}





                                
                            </div>


                            <h5 class="text-danger">{{message}}</h5>


                    </div>
                </div>

                <div class="col-lg-5 col-md-12">

                    <div class="row">
                        <h4 class="text-success">Payment method</h4>
                        <div class="col-sm-12 border bg-light">

                            <div class="mt-4">
                                <h6 class="text-warning">Total Amount : <span class="text-success"> {{sum}}/-</span></h6>
                            </div>
                            <div class="pt-4">
                                <input type="radio" value="COD" checked="checked" name="userpaymentmethod" id="phone_number"
                                    required>&nbsp;&nbsp;&nbsp;<label>COD</label>
                            </div>
                            <div class="pt-4">
                                <input type="radio" value="Online" name="userpaymentmethod" id="phone_number"
                                    required>&nbsp;&nbsp;&nbsp;<label>Online</label>
                            </div>
                            <div class="pt-4">
                                <input type="radio" value="Other" name="userpaymentmethod" id="phone_number"
                                    required>&nbsp;&nbsp;&nbsp;<label>Other</label>
                            </div>

                            <div class="col-lg-12 col-md-12 m-4">
                                   
                                       <button type="submit" class="btn btn-primary">Check out <i
                                            class="flaticon-right-chevron"></i></button>
                            </div>

                        </div>
                    </div>
                            
                   
                </div>
                             
                 </form>
